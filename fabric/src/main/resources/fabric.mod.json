  },
  "license": "MIT",
  "icon": "assets/searchablecontainers/SearchableContainersLogo.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.loucaskreger.searchablecontainers.fabric.SearchableContainersFabric"
    ]
  },
  "mixins": [
    "searchablecontainers-common.mixins.json"
  ],
  "depends": {
    "minecraft": ">=1.19"
  }
}